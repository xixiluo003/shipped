
<%= form_for @boat do |f| %>
  <%= f.label :name %><%= f.text_field :name %><br/>
  <%= f.label :capacity %><%= f.text_field :capacity %><br/>
  <%= f.label :location %><%= f.text_field :location %><br/>
  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= f.label :image %>
  <%= f.file_field :image %><br/>


  <%= f.submit %>
<% end %>
